{
    "name": "pr-reactions",
    "description": "Adds pull request reactions to the pull request list on github.com",
    "version": "0.1",
    "applications": {
        "gecko": {
            "id": "jsnjack@gmail.com",
            "strict_min_version": "48.0"
        }
    },
    "content_scripts": [
        {
            "matches": [
                "*://github.com/*"
            ],
            "js": [
                "pr-reactions.js"
            ],
            "css": [
                "style.css"
            ]
        }
    ],
    "options_ui": {
        "page": "options.html"
    },
    "permissions": [
        "storage"
    ],
    "manifest_version": 2
}